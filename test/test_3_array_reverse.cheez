int main(){
    int[] a = [1,2,3];
    // two ptr reverse
    int l = 0;
    int r = 2;

    
    while (l < r)
    {
        int temp = a[l];
        a[l] = a[r];
        a[r] = temp;
        l = l + 1;
        r = r - 1;
    }
    
    // show array
    
    for (int i = 0; i < 3; i = i + 1)
    {
        printi(a[i]);
    }
    

    return 0;
}
